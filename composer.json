{
    "name": "bristol-su/playground",
    "type": "project",
    "description": "A playground for experimenting and developing modules",
    "minimum-stability": "dev",
    "keywords": [
        "portal",
        "bristol su",
        "playground"
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.2",
        "bristol-su/support": "^1.0",
        "bristol-su/upload-file": "dev-master",
        "bristol-su/typeform": "dev-master",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "itsgoingd/clockwork": "^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "BristolSU\\Playground\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BristolSU\\Playground\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true
}
