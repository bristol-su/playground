{
    "name": "bristol-su/playground",
    "type": "project",
    "description": "A playground for experimenting with and developing modules",
    "minimum-stability": "dev",
    "keywords": [
        "portal",
        "bristol su",
        "playground"
    ],
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.2",
        "bristol-su/static-page": "^1.0",
        "bristol-su/support": "^3.0.2",
        "bristol-su/typeform": "^1.0",
        "bristol-su/upload-file": "^1.0",
        "fideloper/proxy": "^4.0",
        "laravel/framework": "^6.2",
        "laravel/tinker": "^2.0"
    },
    "require-dev": {
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "laravel/ui": "^1.1",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0",
        "itsgoingd/clockwork": "^4.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "BristolSU\\Playground\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "BristolSU\\Playground\\Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "repositories": [
        {
            "type": "path",
            "url": "./../support"
        }
    ]
}
